<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:baselineAligned="false">




    <TextView
        android:text="Selezionare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="32dp" />
    <Button
        android:text="Esporta elenchi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="esportaElenchi"
        android:id="@+id/button5"
        android:layout_below="@+id/textView"
        android:layout_marginStart="16dp"
        android:layout_marginTop="11dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button5"
        android:layout_marginStart="15dp"
        android:layout_toEndOf="@+id/button5"
        android:text="Includi QuantitÃ " />
    <CheckBox
        android:id="@+id/checkBoxPrezzi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/checkBox"
        android:layout_marginStart="15dp"
        android:layout_toEndOf="@+id/button5"
        android:text="Includi Prezzi" />

    <Button
        android:text="Esporta Costi Prodotti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="esportaCosti"
        android:id="@+id/buttonCostiProd"
        android:layout_below="@+id/checkBoxPrezzi"
        android:layout_marginStart="16dp"
        android:layout_marginTop="11dp" />

</RelativeLayout>

